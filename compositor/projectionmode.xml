<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
    "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node name="/com/trollech/examples/car">
    <interface name="com.windriver.automotive.ProjectionMode">
        <method name="returnToNative"/>
        <signal name="touchEvent">
              <arg name="id" type="i" direction="out"/>
              <arg name="x" type="i" direction="out"/>
              <arg name="y" type="i" direction="out"/>
              <arg name="pressed" type="i" direction="out"/>
        </signal>
        <signal name="keyEvent">
              <arg name="keycode" type="i" direction="out"/>
              <arg name="pressed" type="i" direction="out"/>
        </signal>
        <signal name="focusEvent">
              <arg name="acquired" type="b" direction="out"/>
        </signal>


    </interface>
</node>
